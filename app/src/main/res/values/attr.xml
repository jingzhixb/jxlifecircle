<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference"/>
        <attr name="navigateTabSelectedTextColor" format="color|reference"/>
        <attr name="containerId" format="integer|reference"/>
        <attr name="navigateTabTextSize" format="dimension|reference"/>
    </declare-styleable>
    <!--PlayLoadIng-->
    <declare-styleable name="LoadingView">
        <attr name="outerCircleRadius" format="dimension|reference" />
        <attr name="outerCircleColor" format="color|reference" />
        <attr name="innerTriangleRadius" format="dimension|reference" />
        <attr name="innerTriangleColor" format="color|reference" />
        <attr name="backgroundColor" format="color|reference" />
        <attr name="isNeedBackground" format="boolean" />
        <attr name="strokeWidths" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="HeartLayout">
        <attr name="initX" format="dimension" />
        <attr name="initY" format="dimension" />
        <attr name="xRand" format="dimension" />
        <attr name="animLengthRand" format="dimension" />
        <attr name="xPointFactor" format="dimension" />
        <attr name="animLength" format="dimension" />
        <attr name="heart_width" format="dimension" />
        <attr name="heart_height" format="dimension" />
        <attr name="bezierFactor" format="integer" />
        <attr name="anim_duration" format="integer" />
    </declare-styleable>
//圆图
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
</resources>